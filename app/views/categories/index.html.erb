<%= render partial: 'search', locals: { categories: @categories } %>
<% if current_user.grader? %>
    <%= render partial: 'csv_import' %>
<% end %>
<div class="row">
  <div class="col s12">
    <div class="card">
      <div class="card-content">
        <div class="table-responsive m-b-20">
          <table class="">
            <thead>
            <%= render 'thead' %>
            </thead>
            <tbody>
            <%= render partial: 'tbody', locals: { categories: @categories } %>
            </tbody>
          </table>
        </div>
        <%= paginate @categories %>
      </div>
    </div>
  </div>
</div>
